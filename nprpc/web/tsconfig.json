{
	"compilerOptions": {
		"declaration": true,
		"sourceMap": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"outDir": "nprpc",
		"noImplicitAny": true,
		"module": "commonjs",
		"target": "esnext",
		"typeRoots": [ "./node_modules/@types", "./types" ],
		"stripInternal": true
	},
	"include": [
			"./src/**/*"
	],
	"exclude": ["node_modules", "typings"]
}